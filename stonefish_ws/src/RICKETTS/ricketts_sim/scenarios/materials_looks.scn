    <materials>
		<material name="Neutral" density="1000.0" restitution="0.5"/>

		<!-- Subsea Buoyancy Foam: R-3318 -->
		<material name="Foam" density="40.0" restitution="0.3"/>
		<!-- Fake Buoyancy Foam -->
		<material name="InternalFoam" density="30.0" restitution="0.3"/>
		<!-- BlueROV2 Base Frames -->
		<material name="Polyethylene" density="400.0" restitution="0.75"/>
		<!-- BlueROV2 Thrusters Duct -->
		<material name="Polycarbonate" density="435.0" restitution="0.75"/>
		<!-- BlueROV2 Tubes -->
		<material name="Acrylic" density="1100.0" restitution="0.9"/>

		<material name="Ballast" density="570.0" restitution="0.75"/>

		<friction_table>
			<friction material1="Neutral" material2="Neutral" static="0.5" dynamic="0.2"/>
			<friction material1="Neutral" material2="Foam" static="0.5" dynamic="0.2"/>
			<friction material1="Neutral" material2="Polyethylene" static="0.5" dynamic="0.2"/>
			<friction material1="Neutral" material2="Polycarbonate" static="0.5" dynamic="0.2"/>
			<friction material1="Neutral" material2="Acrylic" static="0.5" dynamic="0.2"/>
			<friction material1="Neutral" material2="InternalFoam" static="0.5" dynamic="0.2"/>
			<friction material1="Neutral" material2="Ballast" static="0.5" dynamic="0.2"/>

			<friction material1="Foam" material2="Foam" static="0.5" dynamic="0.2"/>
			<friction material1="Foam" material2="Acrylic" static="0.5" dynamic="0.2"/>
			<friction material1="Foam" material2="Polyethylene" static="0.5" dynamic="0.2"/>
			<friction material1="Foam" material2="Polycarbonate" static="0.5" dynamic="0.2"/>
			<friction material1="Foam" material2="Acrylic" static="0.5" dynamic="0.2"/>
			<friction material1="Foam" material2="InternalFoam" static="0.5" dynamic="0.2"/>
			<friction material1="Foam" material2="Ballast" static="0.5" dynamic="0.2"/>

			<friction material1="Polyethylene" material2="Polyethylene" static="0.5" dynamic="0.2"/>
			<friction material1="Polyethylene" material2="Polycarbonate" static="0.5" dynamic="0.2"/>	
			<friction material1="Polyethylene" material2="Acrylic" static="0.5" dynamic="0.2"/>
			<friction material1="Polyethylene" material2="InternalFoam" static="0.5" dynamic="0.2"/>
			<friction material1="Polyethylene" material2="Ballast" static="0.5" dynamic="0.2"/>

			<friction material1="Acrylic" material2="Acrylic" static="0.5" dynamic="0.2"/>
			<friction material1="Acrylic" material2="InternalFoam" static="0.5" dynamic="0.2"/>
			<friction material1="Acrylic" material2="Ballast" static="0.5" dynamic="0.2"/>

			<friction material1="InternalFoam" material2="InternalFoam" static="0.5" dynamic="0.2"/>
			<friction material1="InternalFoam" material2="Ballast" static="0.5" dynamic="0.2"/>

			<friction material1="Ballast" material2="Ballast" static="0.5" dynamic="0.2"/>
		</friction_table>
	</materials>

    <looks>
        <look name="black" gray="0.01" roughness="0.2" metalness="0.0"/>
		<look name="blue" rgb="0.0 0.3 0.8" roughness="0.0"/>
        <look name="yellow" rgb="1.0 0.9 0.0" roughness="0.3"/>
        <look name="gray" gray="1" roughness="0.2" metalness="0.0"/>
